{
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "env": {
        "node": true
    },
    "rules": {
        "no-console":"off",
        "no-undef":"off",
        "no-plusplus":"off",
        "no-unused-vars":"off",
        "func-names":"off"
    },
    "plugins": [
        "prettier"
    ]
}
