{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-plusplus": "off",
    "prefer-promise-reject-errors": "off",
    "func-names": "off",
    "consistent-return": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off"
  },
  "plugins": ["prettier"]
}
